{
  "name": "@z3phyro/tyres-core",
  "version": "0.1.0",
  "description": "Core project of CLI inspired by prisma, to create and mantain statically typed resources on any typescript project.",
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.44.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "vitest": "^0.32.2"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && npx tsc -p tsconfig.json",
    "postbuild": "echo '#!/usr/bin/env node' | cat - lib/index.js > temp && mv temp lib/index.js",
    "test": "vitest --no-threads --no-watch",
    "ui": "vitest --ui --no-threads",
    "prepare": "husky install",
    "prettier:format": "prettier . -w",
    "prettier:check": "prettier . --check",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "keywords": [
    "translation",
    "typed",
    "i18n",
    "typescript",
    "javascript",
    "library",
    "react",
    "svelte",
    "vue",
    "solidjs"
  ],
  "author": "@z3phyro Maykel LÃ³pez Meneses",
  "license": "ISC"
}
