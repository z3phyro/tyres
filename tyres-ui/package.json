{
  "name": "@z3phyro/tyres-ui",
  "description": "UI for typed resources package (TyRes). It allows to use most of the functions of tyres-core, including, initializing the project.",
  "version": "0.1.9",
  "type": "module",
  "scripts": {
    "dev": "vinxi dev --port 8123",
    "build": "vinxi build",
    "start": "vinxi start",
    "prettier:format": "prettier . -w",
    "postbuild": "echo '#!/usr/bin/env node' | cat - .output/server/index.mjs > temp && mv temp .output/server/index.mjs",
    "prettier:check": "prettier . --check",
    "eslint:check": "eslint .",
    "eslint:fix": "eslint . --fix",
    "fix": "prettier . -w && eslint . --fix",
    "test": "vitest"
  },
  "files": [
    ".output"
  ],
  "license": "ISC",
  "bin": {
    "tyres-ui": "./.output/server/index.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/z3phyro/tyres.git"
  },
  "dependencies": {
    "@kobalte/core": "0.13.0",
    "@modular-forms/solid": "^0.20.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.13.3",
    "@solidjs/start": "^1.0.0",
    "@z3phyro/tyres-core": "^0.1.5",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "solid-js": "^1.8.17",
    "tailwindcss": "^3.4.3",
    "valibot": "^0.13.1",
    "vinxi": "^0.3.11"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "solid-prevent-scroll": "file:./custom_modules/solid-prevent-scroll",
    "@solidjs/testing-library": "^0.8.8",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/user-event": "^14.5.2",
    "jsdom": "^24.1.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  }
}
